{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df1203"
		},
		"LS_AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_AzureDataLakeStorage1'"
		},
		"LS_OracleDB_Dept_DESTN_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_OracleDB_Dept_DESTN'"
		},
		"LS_error_Log_CSVTODB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_error_Log_CSVTODB'"
		},
		"Ls_AzureSynapseAnalyticsDW_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Ls_AzureSynapseAnalyticsDW'"
		},
		"Ls_DLStorage_Gen2_output_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Ls_DLStorage_Gen2_output'"
		},
		"Ls_FileSystemSource_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Ls_FileSystemSource'"
		},
		"Ls_oracledb_source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Ls_oracledb_source'"
		},
		"LS_AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gen2storage1203.dfs.core.windows.net"
		},
		"Ls_DLStorage_Gen2_output_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gen2storage1203.dfs.core.windows.net"
		},
		"Ls_FileSystemSource_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "G:\\Azure"
		},
		"Ls_FileSystemSource_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "rrohitjain12@gmail.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('LS_AzureDataLakeStorage1_accountKey')]"
					},
					"LS_error_Log_CSVTODB_connectionString": {
						"value": "[parameters('LS_error_Log_CSVTODB_connectionString')]"
					},
					"Ls_AzureSynapseAnalyticsDW_connectionString": {
						"value": "[parameters('Ls_AzureSynapseAnalyticsDW_connectionString')]"
					},
					"Ls_DLStorage_Gen2_output_accountKey": {
						"value": "[parameters('Ls_DLStorage_Gen2_output_accountKey')]"
					},
					"LS_OracleDB_Dept_DESTN_connectionString": {
						"value": "[parameters('LS_OracleDB_Dept_DESTN_connectionString')]"
					},
					"Ls_FileSystemSource_password": {
						"value": "[parameters('Ls_FileSystemSource_password')]"
					},
					"Ls_oracledb_source_connectionString": {
						"value": "[parameters('Ls_oracledb_source_connectionString')]"
					},
					"LS_AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('LS_AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"Ls_DLStorage_Gen2_output_properties_typeProperties_url": {
						"value": "[parameters('Ls_DLStorage_Gen2_output_properties_typeProperties_url')]"
					},
					"Ls_FileSystemSource_properties_typeProperties_host": {
						"value": "[parameters('Ls_FileSystemSource_properties_typeProperties_host')]"
					},
					"Ls_FileSystemSource_properties_typeProperties_userId": {
						"value": "[parameters('Ls_FileSystemSource_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}