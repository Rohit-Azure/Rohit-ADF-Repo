					"sinks": [
						{
							"dataset": {
								"referenceName": "Output_ADL_Target_DS",
								"type": "DatasetReference"
							},
							"name": "ITEmpRecords"
						},
						{
							"dataset": {
								"referenceName": "Output_ADL_Target_DS",
								"type": "DatasetReference"
							},
							"name": "ExecuteEmpDetails"
						},
